[globalString = IENV:HTTP_HOST = {$develop_domain}]
    ## CSS and JS Compression ##
    config {
        compressJs = 1
        compressCss = 1
        concatenateCss = 1
        concatenateJs = 1
        concatenateJsAndCss = 1
        inlineStyle2TempFile = 1
        moveJsFromHeaderToFooter = 1
    }

    config.absRefPrefix = auto
[END]